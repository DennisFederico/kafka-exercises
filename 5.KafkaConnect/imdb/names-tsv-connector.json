{
  "name": "names-tsv-source",
  "config": {
    "connector.class": "com.github.jcustenborder.kafka.connect.spooldir.SpoolDirCsvSourceConnector",
    "tasks.max": 1,
    "input.path": "/home/appuser/datasets",
    "input.file.pattern": "names.basics.tsv",
    "error.path": "home/appuser/datasets/errors",
    "finished.path": "/home/appuser/finished",
    "halt.on.error": false,
    "topic": "imdb-names-topic",
    "schema.generation.enabled": true,
    "csv.first.row.as.header": true,
    "csv.separator.char": 9
  }
}